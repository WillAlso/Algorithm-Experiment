int main()
{
    ifstream fin("input.txt");
    ofstream fout("output.txt");
    int n = 0;
    fin >> n;
    int num = pow(2,n);
    int **a = new int*[num];
    for(int i = 0;i < num;i++)
    {
        a[i] = new int[n];
    }
    return 0;
}
